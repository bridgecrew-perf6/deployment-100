upstream cv-dashboard {
        server localhost:3000;
}

server {
    listen 80;
    listen [::]:80;

    server_name visionicca.com;

    location / {
        proxy_pass http://cv-dashboard;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header Host $host;
        proxy_redirect off;
    }
}
